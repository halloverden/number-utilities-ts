{
  "name": "@halloverden/number-utilities-ts",
  "version": "1.0.0",
  "description": "Typescript library with utility functions for a bunch of numbers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "devbuild": "webpack --progress --colors --config webpack.config.dev.js",
    "start": "npm run devbuild && node ./dist_test/index.js",
    "build": "rimraf dist && webpack --progress --colors --config webpack.config.prod.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.js"
  },
  "keywords": [
    "account-number",
    "kid-number",
    "luhn",
    "mod11",
    "norwegian",
    "organization-number",
    "social-security-number",
    "ssn",
    "validation"
  ],
  "author": "Erich Json Fosse <erich@halloverden.no>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.10",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.7.0",
    "exec": "^0.2.1",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2"
  }
}
